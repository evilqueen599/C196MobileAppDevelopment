<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Term Scheduler"
        android:supportsRtl="true"
        android:theme="@style/Theme.C196Aloufi"
        tools:targetApi="31">

        <receiver
            android:name="com.example.c196MobileAppDevelopment.MyBroadcastReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity
            android:name="com.example.c196MobileAppDevelopment.UserInterface.AddAssessment"
            android:exported="false" />
        <activity
            android:name="com.example.c196MobileAppDevelopment.UserInterface.AddCourse"
            android:exported="false" />
        <activity
            android:name="com.example.c196MobileAppDevelopment.UserInterface.AddTerm"
            android:exported="false" />
        <activity
            android:name="com.example.c196MobileAppDevelopment.UserInterface.AssessmentsList"
            android:exported="false" />
        <activity
            android:name="com.example.c196MobileAppDevelopment.UserInterface.CourseList"
            android:exported="false" />
        <activity
            android:name="com.example.c196MobileAppDevelopment.UserInterface.DetailedTerm"
            android:exported="false" />
        <activity
            android:name="com.example.c196MobileAppDevelopment.UserInterface.mainScreen"
            android:exported="false"
            android:parentActivityName="com.example.c196MobileAppDevelopment.UserInterface.mainScreen" />
        <activity
            android:name="com.example.c196MobileAppDevelopment.UserInterface.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>